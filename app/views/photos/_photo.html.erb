<div class="card">
  <%= image_tag photo.url, class: "card-img-top" %>
  <div class="card-block">
    <h4 class="card-title"><%= photo.username %></h4>
    <p class="card-text"><%= photo.caption %></p>
    <a href="#" class="btn btn-primary"><%= pluralize(photo.likes_count, 'like') %></a>
  </div>
</div>